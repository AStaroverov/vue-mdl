{
  "name": "vue-mdl",
  "version": "0.7.4",
  "description": "Reusable Vue components using Material Design Lite",
  "main": "dist/vue-mdl.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:e2e": "./build/e2e.sh",
    "test:unit": "./build/unit.sh",
    "test:chrome": "./build/e2e.sh chrome",
    "test:firefox": "./build/e2e.sh firefox",
    "build": "npm run build:dev && npm run build:prod",
    "build:prod": "webpack --bail --progress --hide-modules --config build/webpack.dist.prod.conf.js",
    "build:dev": "webpack --bail --progress --hide-modules --config build/webpack.dist.dev.conf.js",
    "dev": "node build/dev-server.js",
    "dev:e2e": "node build/e2e-server.js",
    "dev:doc": "node build/doc-server.js",
    "doc": "webpack -p --progress --bail --config build/webpack.gh-pages.conf.js"
  },
  "keywords": [
    "vue",
    "mdl",
    "ui",
    "desgin",
    "google",
    "material",
    "lite",
    "bootstrap",
    "vuestrap",
    "vuemdl",
    "front",
    "normalize",
    "css"
  ],
  "author": "posva",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^1.0.21"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.5",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "chromedriver": "2.21.2",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-friendly-formatter": "^2.0.4",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "highlight.js": "^9.3.0",
    "html-webpack-plugin": "^2.16.0",
    "inject-loader": "^2.0.1",
    "jade": "^1.11.0",
    "jquery": "^2.2.3",
    "lodash": "^4.11.1",
    "material-design-lite": "^1.1.3",
    "mocha": "^2.4.5",
    "mocha-phantomjs": "^4.0.2",
    "nightwatch": "^0.8.18",
    "node-sass": "^3.6.0",
    "sass-loader": "^3.2.0",
    "selenium-server": "^2.53.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "vue": "^1.0.21",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.2.3",
    "vue-style-loader": "^1.0.0",
    "vue-transfer-dom": "^1.1.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-fail-plugin": "^1.0.4",
    "webpack-hot-middleware": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-mdl.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-mdl/issues"
  },
  "homepage": "https://github.com/posva/vue-mdl#readme"
}
